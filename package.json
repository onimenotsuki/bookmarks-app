{
  "name": "bookmarks-app",
  "version": "1.0.0",
  "description": "Bookmarks application with localStorage, sass and parcel bundler",
  "main": "src/index.js",
  "scripts": {
    "start": "./node_modules/.bin/parcel src/index.html -p 3000",
    "build": "./node_modules/.bin/parcel build src/index.js --no-cache",
    "build:cache": "./node_modules/.bin/parcel build src/index.js"
  },
  "keywords": [
    "es6",
    "es2015",
    "parceljs"
  ],
  "engines": {
    "node": "~8.9.4"
  },
  "author": "Edgar Talledos <onimenotsuki@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^8.0.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2",
    "postcss-modules": "^1.1.0",
    "uikit": "^3.0.0-beta.40",
    "airbnb-browser-shims": "^2.1.0"
  },
  "devDependencies": {

    "babel-preset-airbnb": "^2.4.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "sass-lint": "^1.12.1"
  }
}
